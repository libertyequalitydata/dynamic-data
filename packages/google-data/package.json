{
  "name": "@dynamic-data/google-data",
  "version": "0.0.2",
  "description": "Mockup Data for Google connector",
  "author": "tero <tero@prifina.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": "./dist/esm/index.js",
    "./mockups": {
      "import": "./dist/esm/mockups/index.js",
      "require": "./dist/cjs/mockups/index.js"
    },
    "./mockups/*": {
      "import": "./dist/esm/mockups/*.js",
      "require": "./dist/cjs/mockups/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf ./dist && yarn build:esm && yarn build:cjs",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --out-dir ./dist/esm",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --out-dir ./dist/cjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1"
  }
}
